<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="560dp"
    tools:context=".inputbstk_sesudah.Inputbstk_1">
    <LinearLayout
        android:id="@+id/line"
        android:layout_width="match_parent"
        android:layout_marginTop="10dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/ta1"
            android:layout_width="25dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:text="No"
            android:textAlignment="center"
            android:layout_marginBottom="5dp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/ta2"
            android:layout_width="226dp"
            android:layout_height="20dp"
            android:layout_toRightOf="@id/ta1"
            android:text="Perlengkapan"
            android:layout_marginBottom="5dp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/ta3"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:layout_toRightOf="@+id/ta2"
            android:text="B"
            android:layout_marginRight="1dp"
            android:layout_marginBottom="5dp"
            android:textAlignment="center"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/ta4"
            android:layout_width="32dp"
            android:layout_height="20dp"
            android:layout_toRightOf="@+id/ta3"
            android:text="R"
            android:layout_marginBottom="5dp"
            android:layout_marginRight="2dp"
            android:textAlignment="center"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/ta5"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:layout_toRightOf="@+id/ta4"
            android:text="T"
            android:layout_marginBottom="5dp"
            android:textAlignment="center"
            android:textStyle="bold" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/line1"
        android:layout_below="@+id/line"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tb1"
            android:layout_width="25dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:text="1"
            android:layout_below="@+id/ta1"
            android:textAlignment="center" />
        <TextView
            android:id="@+id/tb2"
            android:layout_width="225dp"
            android:layout_height="20dp"
            android:layout_toRightOf="@id/tb1"
            android:layout_below="@+id/ta2"
            android:text="Automatic Light Switch" />
        <RadioGroup
            android:id="@+id/rgAutomatic_Light_Switch"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_toRightOf="@id/tb2"
            android:layout_below="@+id/ta3">
            <RadioButton
                android:id="@+id/b1"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_alignTop="@+id/rgAutomatic_Light_Switch"
                android:checked="true"
                android:textColor="@android:color/transparent" />
            <RadioButton
                android:id="@+id/r1"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:layout_alignTop="@+id/rgAutomatic_Light_Switch"
                android:checked="true" />
            <RadioButton
                android:id="@+id/t1"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:checked="true"/>
        </RadioGroup>
    </LinearLayout>

    <EditText
        android:id="@+id/teAutomatic_Light_Switch"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="35dp"
        android:layout_below="@+id/line1"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />
    <LinearLayout
        android:id="@+id/line2"
        android:layout_below="@+id/teAutomatic_Light_Switch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tc1"
            android:layout_width="25dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:text="2"
            android:layout_below="@+id/teAutomatic_Light_Switch"
            android:textAlignment="center" />
        <TextView
            android:id="@+id/tc2"
            android:layout_width="225dp"
            android:layout_height="20dp"
            android:layout_toRightOf="@id/tc1"
            android:layout_below="@+id/teAutomatic_Light_Switch"
            android:text="Lampu Kecil" />
        <RadioGroup
            android:id="@+id/rgLampu_Kecil"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_toRightOf="@id/tc2"
            android:layout_below="@+id/teAutomatic_Light_Switch">
            <RadioButton
                android:id="@+id/b2"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
            <RadioButton
                android:id="@+id/r2"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
            <RadioButton
                android:id="@+id/t2"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
        </RadioGroup>
    </LinearLayout>

    <EditText
        android:id="@+id/teLampu_Kecil"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line2"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

    <LinearLayout
        android:id="@+id/line3"
        android:layout_below="@+id/teLampu_Kecil"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/td1"
            android:layout_width="25dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:text="3"
            android:layout_below="@+id/teLampu_Kecil"
            android:textAlignment="center" />
        <TextView
            android:id="@+id/td2"
            android:layout_width="225dp"
            android:layout_height="20dp"
            android:layout_toRightOf="@id/td1"
            android:layout_below="@+id/teLampu_Kecil"
            android:text="Lampu Dekat" />
        <RadioGroup
            android:id="@+id/rgLampu_Dekat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_toRightOf="@id/td2"
            android:layout_below="@+id/teLampu_Kecil">

            <RadioButton
                android:id="@+id/b3"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
            <RadioButton
                android:id="@+id/r3"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true" />
            <RadioButton
                android:id="@+id/t3"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true" />
        </RadioGroup>
    </LinearLayout>

    <EditText
        android:id="@+id/teLampu_Dekat"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line3"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

    <LinearLayout
        android:id="@+id/line4"
        android:layout_below="@+id/teLampu_Dekat"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/te1"
            android:layout_width="25dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:text="4"
            android:layout_below="@+id/teLampu_Dekat"
            android:textAlignment="center" />
        <TextView
            android:id="@+id/te2"
            android:layout_width="225dp"
            android:layout_height="20dp"
            android:layout_toRightOf="@id/te1"
            android:layout_below="@+id/teLampu_Dekat"
            android:text="Lampu Jauh" />

        <RadioGroup
            android:id="@+id/rgLampu_Jauh"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_toRightOf="@id/te2"
            android:layout_below="@+id/teLampu_Dekat">
            <RadioButton
                android:id="@+id/b4"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
            <RadioButton
                android:id="@+id/r4"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
            <RadioButton
                android:id="@+id/t4"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
        </RadioGroup>
    </LinearLayout>

    <EditText
        android:id="@+id/teLampu_Jauh"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line4"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

    <LinearLayout
        android:id="@+id/line5"
        android:layout_below="@+id/teLampu_Jauh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tf1"
            android:layout_width="25dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:text="5"
            android:layout_below="@+id/teLampu_Jauh"
            android:textAlignment="center" />
        <TextView
            android:id="@+id/tf2"
            android:layout_width="225dp"
            android:layout_height="20dp"
            android:layout_toRightOf="@id/tf1"
            android:layout_below="@+id/teLampu_Jauh"
            android:text="Lampu Kabut / Fog Lamp" />
        <RadioGroup
            android:id="@+id/rgLampu_Kabut_Fog_Lamp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_toRightOf="@id/tf2"
            android:layout_below="@+id/teLampu_Jauh">
            <RadioButton
                android:id="@+id/b5"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
            <RadioButton
                android:id="@+id/r5"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
            <RadioButton
                android:id="@+id/t5"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
        </RadioGroup>
    </LinearLayout>

    <EditText
        android:id="@+id/teLampu_Kabut_Fog_Lamp"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line5"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

    <LinearLayout
        android:id="@+id/line6"
        android:layout_below="@+id/teLampu_Kabut_Fog_Lamp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/tg1"
            android:layout_width="25dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:text="6"
            android:layout_below="@+id/teLampu_Kabut_Fog_Lamp"
            android:textAlignment="center" />
        <TextView
            android:id="@+id/tg2"
            android:layout_width="225dp"
            android:layout_height="20dp"
            android:layout_toRightOf="@id/tg1"
            android:layout_below="@+id/teLampu_Kabut_Fog_Lamp"
            android:text="Lampu Sign Depan" />
        <RadioGroup
            android:id="@+id/rgLampu_Sign_Depan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_toRightOf="@id/tg2"
            android:layout_below="@+id/teLampu_Kabut_Fog_Lamp">
            <RadioButton
                android:id="@+id/b6"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
            <RadioButton
                android:id="@+id/r6"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
            <RadioButton
                android:id="@+id/t6"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
        </RadioGroup>
    </LinearLayout>

    <EditText
        android:id="@+id/teLampu_Sign_Depan"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line6"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

    <LinearLayout
        android:id="@+id/line7"
        android:layout_below="@+id/teLampu_Sign_Depan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/th1"
            android:layout_width="25dp"
            android:layout_height="20dp"
            android:layout_marginLeft="10dp"
            android:text="7"
            android:layout_below="@+id/teLampu_Sign_Depan"
            android:textAlignment="center" />
        <TextView
            android:id="@+id/th2"
            android:layout_width="225dp"
            android:layout_height="20dp"
            android:layout_toRightOf="@id/th1"
            android:layout_below="@+id/teLampu_Sign_Depan"
            android:text="Lampu Sign Belakang" />
        <RadioGroup
            android:id="@+id/rgLampu_Sign_Belakang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_toRightOf="@id/th2"
            android:layout_below="@+id/teLampu_Sign_Depan">
            <RadioButton
                android:id="@+id/b7"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
            <RadioButton
                android:id="@+id/r7"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>
            <RadioButton
                android:id="@+id/t7"
                android:layout_width="30dp"
                android:layout_height="20dp"
                android:checked="true"/>

        </RadioGroup>
    </LinearLayout>

    <EditText
        android:id="@+id/teLampu_Sign_Belakang"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line7"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

    <ImageButton
        android:id="@+id/button_next"
        android:layout_below="@+id/teLampu_Sign_Belakang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/button_next"
        android:layout_marginLeft="290dp" />

    <ImageButton
        android:id="@+id/button_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/teLampu_Sign_Belakang"
        android:layout_marginLeft="15dp"
        android:background="@drawable/button_back" />
</RelativeLayout>