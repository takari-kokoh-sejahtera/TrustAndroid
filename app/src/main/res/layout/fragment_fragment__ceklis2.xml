<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="560dp"
    tools:context=".inputbstk_sebelum.Fragment_Ceklis2">

<LinearLayout
    android:id="@+id/line"
    android:layout_marginTop="10dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/ta1"
        android:layout_width="25dp"
        android:layout_height="20dp"
        android:layout_marginLeft="10dp"
        android:text="No"
        android:textAlignment="center"
        android:layout_marginBottom="5dp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/ta2"
        android:layout_width="226dp"
        android:layout_height="20dp"
        android:layout_toRightOf="@id/ta1"
        android:text="Perlengkapan"
        android:layout_marginBottom="5dp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/ta3"
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:layout_toRightOf="@+id/ta2"
        android:text="B"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="1dp"
        android:textAlignment="center"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/ta4"
        android:layout_width="32dp"
        android:layout_height="20dp"
        android:layout_toRightOf="@+id/ta3"
        android:text="R"
        android:layout_marginBottom="5dp"
        android:layout_marginRight="2dp"
        android:textAlignment="center"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/ta5"
        android:layout_width="30dp"
        android:layout_height="20dp"
        android:layout_toRightOf="@+id/ta4"
        android:text="T"
        android:layout_marginBottom="5dp"
        android:textAlignment="center"
        android:textStyle="bold" />
</LinearLayout>

<LinearLayout
    android:id="@+id/line1"
    android:layout_below="@+id/line"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tb1"
        android:layout_width="25dp"
        android:layout_height="20dp"
        android:layout_marginLeft="10dp"
        android:text="8"
        android:layout_below="@+id/ta1"
        android:textAlignment="center" />
    <TextView
        android:id="@+id/tb2"
        android:layout_width="225dp"
        android:layout_height="20dp"
        android:layout_toRightOf="@id/tb1"
        android:layout_below="@+id/ta2"
        android:text="Lampu Belakang" />
    <RadioGroup
        android:id="@+id/rgLampu_Belakang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_toRightOf="@id/tb2"
        android:layout_below="@+id/ta3">
        <RadioButton
            android:id="@+id/b8"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:checked="true" />
        <RadioButton
            android:id="@+id/r8"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/t8"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:checked="true"/>
    </RadioGroup>
</LinearLayout>

    <EditText
        android:id="@+id/teLampu_Belakang"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line1"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

<LinearLayout
    android:id="@+id/line2"
    android:layout_below="@+id/teLampu_Belakang"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tc1"
        android:layout_width="25dp"
        android:layout_height="20dp"
        android:layout_marginLeft="10dp"
        android:text="9"
        android:layout_below="@+id/teLampu_Belakang"
        android:textAlignment="center" />
    <TextView
        android:id="@+id/tc2"
        android:layout_width="225dp"
        android:layout_height="20dp"
        android:layout_toRightOf="@id/tc1"
        android:layout_below="@+id/teLampu_Belakang"
        android:text="Lampu Rem" />
    <RadioGroup
        android:id="@+id/rgLampu_Rem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_toRightOf="@id/tc2"
        android:layout_below="@+id/teLampu_Belakang">
        <RadioButton
            android:id="@+id/b9"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/r9"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/t9"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
    </RadioGroup>
</LinearLayout>

    <EditText
        android:id="@+id/teLampu_Rem"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line2"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

<LinearLayout
    android:id="@+id/line3"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_below="@id/teLampu_Rem"
    android:layout_marginTop="1dp">
    <TextView
        android:id="@+id/td1"
        android:layout_width="25dp"
        android:layout_height="20dp"
        android:layout_below="@+id/teLampu_Rem"
        android:layout_marginLeft="10dp"
        android:text="10"
        android:textAlignment="center" />
    <TextView
        android:id="@+id/td2"
        android:layout_width="225dp"
        android:layout_height="20dp"
        android:layout_below="@+id/teLampu_Rem"
        android:layout_toRightOf="@id/td1"
        android:text="Lampu Mundur" />
    <RadioGroup
        android:id="@+id/rgLampu_Mundur"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/teLampu_Rem"
        android:layout_toRightOf="@id/td2"
        android:orientation="horizontal">
        <RadioButton
            android:id="@+id/b10"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true" />
        <RadioButton
            android:id="@+id/r10"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true" />
        <RadioButton
            android:id="@+id/t10"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true" />
    </RadioGroup>
</LinearLayout>

    <EditText
        android:id="@+id/teLampu_Mundur"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line3"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

<LinearLayout
    android:id="@+id/line4"
    android:layout_below="@+id/teLampu_Mundur"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/te1"
        android:layout_width="25dp"
        android:layout_height="20dp"
        android:layout_marginLeft="10dp"
        android:text="11"
        android:layout_below="@+id/teLampu_Mundur"
        android:textAlignment="center" />
    <TextView
        android:id="@+id/te2"
        android:layout_width="225dp"
        android:layout_height="20dp"
        android:layout_toRightOf="@id/te1"
        android:layout_below="@+id/teLampu_Mundur"
        android:text="Lampu Dashboard" />
    <RadioGroup
        android:id="@+id/rgLampu_Dashboard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_toRightOf="@id/te2"
        android:layout_below="@+id/teLampu_Mundur">
        <RadioButton
            android:id="@+id/b11"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/r11"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/t11"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
    </RadioGroup>
</LinearLayout>

    <EditText
        android:id="@+id/teLampu_Dashboard"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line4"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

<LinearLayout
    android:id="@+id/line5"
    android:layout_below="@+id/teLampu_Dashboard"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tf1"
        android:layout_width="25dp"
        android:layout_height="20dp"
        android:layout_marginLeft="10dp"
        android:text="12"
        android:layout_below="@+id/teLampu_Dashboard"
        android:textAlignment="center" />
    <TextView
        android:id="@+id/tf2"
        android:layout_width="225dp"
        android:layout_height="20dp"
        android:layout_toRightOf="@id/tf1"
        android:layout_below="@+id/teLampu_Dashboard"
        android:text="Lampu Plafond Depan dan Belakang" />
    <RadioGroup
        android:id="@+id/rgLampu_Plafond_Depan_dan_Belakang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_toRightOf="@id/tf2"
        android:layout_below="@+id/teLampu_Dashboard">
        <RadioButton
            android:id="@+id/b12"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/r12"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/t12"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
    </RadioGroup>
</LinearLayout>

    <EditText
        android:id="@+id/teLampu_Plafond_Depan_dan_Belakang"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line5"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

<LinearLayout
    android:id="@+id/line6"
    android:layout_below="@+id/teLampu_Plafond_Depan_dan_Belakang"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tg1"
        android:layout_width="25dp"
        android:layout_height="20dp"
        android:layout_marginLeft="10dp"
        android:text="13"
        android:layout_below="@+id/teLampu_Plafond_Depan_dan_Belakang"
        android:textAlignment="center" />
    <TextView
        android:id="@+id/tg2"
        android:layout_width="225dp"
        android:layout_height="20dp"
        android:layout_toRightOf="@id/tg1"
        android:layout_below="@+id/teLampu_Plafond_Depan_dan_Belakang"
        android:text="Klakson" />
    <RadioGroup
        android:id="@+id/rgKlakson"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_toRightOf="@id/tg2"
        android:layout_below="@+id/teLampu_Plafond_Depan_dan_Belakang">
        <RadioButton
            android:id="@+id/b13"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/r13"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/t13"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
    </RadioGroup>
</LinearLayout>

    <EditText
        android:id="@+id/teKlakson"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line6"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

<LinearLayout
    android:id="@+id/line7"
    android:layout_below="@id/teKlakson"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/th1"
        android:layout_width="25dp"
        android:layout_height="20dp"
        android:layout_marginLeft="10dp"
        android:text="14"
        android:layout_below="@+id/teKlakson"
        android:textAlignment="center" />
    <TextView
        android:id="@+id/th2"
        android:layout_width="225dp"
        android:layout_height="20dp"
        android:layout_toRightOf="@id/th1"
        android:layout_below="@+id/teKlakson"
        android:text="Antena" />
    <RadioGroup
        android:id="@+id/rgAntena"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_toRightOf="@id/th2"
        android:layout_below="@+id/teKlakson">
        <RadioButton
            android:id="@+id/b14"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/r14"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/t14"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
    </RadioGroup>
</LinearLayout>

    <EditText
        android:id="@+id/teAntena"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line7"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

    <ImageButton
        android:id="@+id/button_ceklis"
        android:background="@drawable/button_next"
        android:layout_below="@+id/teAntena"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="290dp"/>

    <ImageButton
        android:id="@+id/button_Back"
        android:background="@drawable/button_back"
        android:layout_below="@+id/teAntena"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="15dp"/>

</RelativeLayout>