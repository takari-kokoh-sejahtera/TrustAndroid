<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="560dp"
    tools:context=".inputbstk_sebelum.Fragment_Ceklis4">

 <LinearLayout
     android:id="@+id/line4"
     android:layout_marginTop="10dp"
     android:layout_width="match_parent"
     android:layout_height="wrap_content">
     <TextView
         android:id="@+id/ta1"
         android:layout_width="25dp"
         android:layout_height="20dp"
         android:layout_marginLeft="10dp"
         android:text="No"
         android:textAlignment="center"
         android:layout_marginBottom="5dp"
         android:textStyle="bold" />
     <TextView
         android:id="@+id/ta2"
         android:layout_width="226dp"
         android:layout_height="20dp"
         android:layout_toRightOf="@id/ta1"
         android:text="Perlengkapan"
         android:layout_marginBottom="5dp"
         android:textStyle="bold" />
     <TextView
         android:id="@+id/ta3"
         android:layout_width="30dp"
         android:layout_height="20dp"
         android:layout_toRightOf="@+id/ta2"
         android:text="B"
         android:layout_marginBottom="5dp"
         android:layout_marginRight="1dp"
         android:textAlignment="center"
         android:textStyle="bold" />
     <TextView
         android:id="@+id/ta4"
         android:layout_width="32dp"
         android:layout_height="20dp"
         android:layout_toRightOf="@+id/ta3"
         android:text="R"
         android:layout_marginBottom="5dp"
         android:layout_marginRight="2dp"
         android:textAlignment="center"
         android:textStyle="bold" />
     <TextView
         android:id="@+id/ta5"
         android:layout_width="30dp"
         android:layout_height="20dp"
         android:layout_toRightOf="@+id/ta4"
         android:text="T"
         android:layout_marginBottom="5dp"
         android:textAlignment="center"
         android:textStyle="bold" />
 </LinearLayout>

    <LinearLayout
        android:id="@+id/line41"
        android:layout_below="@+id/line4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tb1"
        android:layout_width="25dp"
        android:layout_height="20dp"
        android:layout_marginLeft="10dp"
        android:text="22"
        android:layout_below="@+id/ta1"
        android:textAlignment="center" />
    <TextView
        android:id="@+id/tb2"
        android:layout_width="225dp"
        android:layout_height="20dp"
        android:layout_toRightOf="@id/tb1"
        android:layout_below="@+id/ta2"
        android:text="Safety Belt" />
    <RadioGroup
        android:id="@+id/rgSafety_Belt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_toRightOf="@id/tb2"
        android:layout_below="@+id/ta3">
        <RadioButton
            android:id="@+id/b22"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:checked="true" />
        <RadioButton
            android:id="@+id/r22"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/t22"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:checked="true"/>
    </RadioGroup>
    </LinearLayout>

    <EditText
        android:id="@+id/teSafety_Belt"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line41"
        android:layout_marginLeft="35dp"
        android:layout_marginBottom="5dp"
        android:gravity="start|top"
        android:hint="Keterangan"
        android:inputType="textMultiLine"
        android:textSize="14sp" />

    -----------------------------------------------------

    <TextView
        android:id="@+id/tc1"
        android:layout_width="25dp"
        android:layout_height="20dp"
        android:layout_marginLeft="10dp"
