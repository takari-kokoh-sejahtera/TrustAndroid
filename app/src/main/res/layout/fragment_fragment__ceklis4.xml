        android:layout_toRightOf="@id/tf2"
        android:layout_below="@+id/teAsbak">
        <RadioButton
            android:id="@+id/b26"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/r26"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/t26"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
    </RadioGroup>
    <EditText
        android:id="@+id/teSarung_Jok"
