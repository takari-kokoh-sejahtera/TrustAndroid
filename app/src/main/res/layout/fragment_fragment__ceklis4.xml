        android:layout_below="@+id/teSafety_Belt"
        android:textAlignment="center" />
    <TextView
        android:id="@+id/tc2"
        android:layout_width="225dp"
        android:layout_height="20dp"
        android:layout_toRightOf="@id/tc1"
        android:layout_below="@+id/teSafety_Belt"
        android:text="Karpet" />

    <RadioGroup
        android:id="@+id/rgKarpet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_toRightOf="@id/tc2"
        android:layout_below="@+id/teSafety_Belt">
        <RadioButton
            android:id="@+id/b23"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/r23"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
        <RadioButton
            android:id="@+id/t23"
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:checked="true"/>
    </RadioGroup>
    <EditText
        android:id="@+id/teKarpet"
        android:layout_width="290dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tc2"
